
extends layout

block content
  h1= title
  p Welcome to Tejaswi Avula #{title}
  button(type="button", class="btn btn-success" onclick="update()") Press me
  div(id="target") Result
  script. 
      var client_count=0 
      function update(){
        client_count++;
        document.getElementById("target").innerHTML=`Value now ${client_count}`
      } 
  a(href="/handbag") Handbag 